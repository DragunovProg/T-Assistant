<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teacherassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".addSubjectActivity"
            android:parentActivityName=".SubjectActivity"/>
        <activity android:name=".AddShedule"
            android:parentActivityName=".ScheduleActivity"/>
        <activity
            android:name=".SubjectActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ScheduleActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ChangerSchedule" />
        <activity
            android:name=".EditSubjects"
            android:parentActivityName=".SubjectActivity"/>
    </application>

</manifest>